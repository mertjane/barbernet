{
  "expo": {
    "name": "barbernet",
    "slug": "barbernet",
    "version": "1.0.2",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "barbernet",
